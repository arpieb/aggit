<h2>Feed Entries</h2>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
<%= for feed_entry <- @page.entries do %>
    <tr>
      <td><%= link feed_entry.title, to: feed_entry_path(@conn, :show, feed_entry) %></td>
      <td><%= if !is_nil(feed_entry.link), do: link("View", to: feed_entry.link, target: "_blank") %></td>
    </tr>
<% end %>
  </tbody>
</table>

<%= pagination_links @conn, @page %>
